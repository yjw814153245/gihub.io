<body class="login-img3-body">
    <div class="container login">
        <div class="row">
            <div class="col-md-4"></div>
            <div class="col-md-4">
                <form action="/indexInsert" method="post" class="login-form">
                  <div class="login-wrap">
                    <div class="input-group">
                      <span class="input-group-addon"><i class="icon_lock_alt"></i></span>
                      <input type="text" class="form-control" name="name" placeholder="注册账号">
                    </div>

                    <div class="input-group">
                      <span class="input-group-addon"><i class="icon_profile"></i></span>
                      <input type="password" class="form-control" name="password" placeholder="注册密码">
                    </div>
                    <div class="input-group">
                      <span class="input-group-addon"><i class="icon_profile"></i></span>
                      <input type="password" class="form-control" name="repassword" placeholder="确认密码">
                    </div>
                    <div class="input-group">
                      <span class="input-group-addon">验证码：</span>
                      <input type="text" class="form-control" name="code" style="height: 44px;">
                      <div class="input-group-addon">
                        <a href=""><img src="/admin/code" alt=""></a>
                      </div>
                    </div>
                    <!-- <label class="checkbox">
                      <input type="checkbox" value="remember-me">记住密码
                      <span class="pull-right"> <a href="#"> 招呼密码?</a></span>
                    </label> -->
                    <!-- <button class="btn btn-primary btn-lg btn-block" type="submit">登录</button> -->
                    <!-- <button class="btn btn-info btn-lg btn-block" href="/admin/register">注册</button> -->
                    <div class="">
                      <input type="submit" class="btn btn-info btn-lg btn-block" value="注册">
                    </div>
                    <div class="center-block">
                      <!-- <input type="submit" class="btn btn-success" value="登录"> -->
                    </div>
                  </div> 
                   <input type="hidden" name="state" value="可用"> 
                </form>                 
            </div>
            <div class="col-md-4"></div>
        </div>
    </div>

